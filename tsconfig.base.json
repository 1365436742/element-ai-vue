{
  "compilerOptions": {
    "outDir": "dist",
    "target": "es2018",
    "module": "esnext",
    "baseUrl": ".",
    "sourceMap": false,
    "moduleResolution": "node",
    "allowJs": true,
    "strict": true,
    "noUnusedLocals": true,
    "resolveJsonModule": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "removeComments": false,
    "rootDir": ".",
    "types": [],
    "paths": {
      "@element-ai-vue/components": ["packages/components"],
      "@element-ai-vue/components/*": ["packages/components/*"],
      "@element-ai-vue/utils": ["packages/utils"],
      "@element-ai-vue/utils/*": ["packages/utils/*"],
      "@element-ai-vue/hooks": ["packages/hooks"],
      "@element-ai-vue/directives": ["packages/directives"],
      "@element-ai-vue/constants": ["packages/constants"],
      "@element-ai-vue/locale": ["packages/locale"],
      "@element-ai-vue/locale/*": ["packages/locale/*"],
      "element-ai-vue": ["packages/element-ai-vue"]
    }
  }
}
